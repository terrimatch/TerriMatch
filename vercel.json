{
  "version": 2,
  "functions": {
    "backend/src/api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/backend/src/api/index.js"
    }
  ],
  "builds": [
    {
      "src": "backend/src/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
