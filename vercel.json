{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram-webhook",
      "dest": "backend/src/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend/src/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
