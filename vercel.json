{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/src/api/index.js"
    }
  ]
}
{
  "routes": [
    {
      "src": "/telegram",
      "dest": "frontend/src/telegram/index.html"
    }
  ]
}
